{
  "name": "@wings-software/harness-telemetry",
  "version": "1.0.0",
  "description": "An analytics wrapper for front end Harness applications",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn; yarn ui:build; yarn ui:build:types; yarn types:copy",
    "postinstall": "yarn husky install",
    "setup": "bash scripts/github-package-registry.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "types:copy": "echo 'Adhoc type copy...'; mv dist/types/src/* dist/; rm -rf dist/types",
    "ui:build": "NODE_ENV=production webpack",
    "ui:build:types": "tsc --emitDeclarationOnly --declarationDir ./dist/",
    "ui:types": "tsc --emitDeclarationOnly --watch --declarationDir ./docs/static/"
  },
  "author": "Nathan Nguyen",
  "license": "UNLICENSED",
  "dependencies": {
    "@segment/analytics.js-core": "4.0.1",
    "@segment/analytics.js-integration-segmentio": "4.2.5",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier --write ."
    ]
  }
}
